<div class="admin-container-form">
<h1><%%= t(:new, :default => "New {{model}}", :model => <%= model_name %>.human_name, :scope => [:railties, :scaffold]) %></h1>

<div class="admin-control">
  <div class="back"><%%= link_to t(:back, :default => 'Back', :scope => [:railties, :scaffold]), <%= plural_route %> %></div>
</div>

<%% cute_admin_form_for(<% if nested_routes? %>[<%= form_nesting %>, @<%= singular_name %>]<% else %>@<%= singular_name %><% end %>) do |f| %>
  <%%= f.error_messages %>
  <%%= render :partial => "form", :locals => {:f => f} %>
  <div class="admin-submit">
    <%%= f.submit t(:create, :default => "Create", :scope => [:railties, :scaffold]) %>
  </div>
<%% end %>
</div>